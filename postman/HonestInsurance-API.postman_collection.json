									"    var jsonData = pm.response.json();",
									"    pm.environment.set(\"PoolContractAdr\", jsonData.PoolContractAdr);",
									"    pm.environment.set(\"BondContractAdr\", jsonData.BondContractAdr);",
									"    pm.environment.set(\"BankContractAdr\", jsonData.BankContractAdr);",
									"    pm.environment.set(\"PolicyContractAdr\", jsonData.PolicyContractAdr);",
									"    pm.environment.set(\"SettlementContractAdr\", jsonData.SettlementContractAdr);",
									"    pm.environment.set(\"AdjustorContractAdr\", jsonData.AdjustorContractAdr);",
									"    pm.environment.set(\"TimerContractAdr\", jsonData.TimerContractAdr);",
